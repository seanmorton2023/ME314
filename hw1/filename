# LHS = LHS.T
# RHS = RHS.T
eqn = sym.Eq(LHS.T, RHS.T)

print("Equation:")
display(eqn)

soln = sym.solve(eqn, qdd, dict = True)

print(soln)

for x in q:
    display(sym.Eq(q, soln[0][x]))
    
