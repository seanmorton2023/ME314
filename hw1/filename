lantern_tr2 = standardize_dates(lantern_tr)

#sort by key, i.e. by date
dutchie_tr3 = co.OrderedDict(sorted(dutchie_tr2.items()))
lantern_tr3 = co.OrderedDict(sorted(lantern_tr3.items()))
                                
