print(soln)

for x in q[0]:
    display(sym.Eq(q, soln[x]))
30/48:
# You can start your implementation here :)

# 8. solve equation and display result
LHS = LHS.T
RHS = RHS.T
eqn = sym.Eq(LHS, RHS)
soln = sym.solve(eqn, q, dict = True)

print(soln)

for x in q:
    display(sym.Eq(q, soln[0][x]))
